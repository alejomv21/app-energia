            android:text="@string/camara" />


        <EditText
            android:id="@+id/EditUltimoLavado"
            android:layout_width="60dp"
            android:layout_height="23dp"
            android:layout_below="@+id/textG"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:layout_toEndOf="@+id/fechaUltimoLavado"
            android:background="@drawable/style_etbg"
            android:gravity="center"
            android:textSize="8sp" />

        <TextView
            android:id="@+id/textI"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/textH"
            android:text="@string/ultimo_tanqueo_gls" />

        <ImageButton
