            android:layout_marginTop="10dp"
            android:background="@color/black"
            android:layout_marginEnd="20dp"
            android:layout_marginStart="20dp"
            android:padding="1dp"
            android:contentDescription="TODO"
            android:src="@android:drawable/divider_horizontal_bright" />

        <TextView
            android:id="@+id/textLogistica"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageViewSeparatorLogistica"
            android:layout_centerHorizontal="true"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:layout_marginTop="20dp"
            android:text="LOGISTICA" />

        <TextView
            android:id="@+id/TextPDuplicado"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/textLogistica"
            android:text="Transporte a Tiempo" />

        <Spinner
            android:id="@+id/spinnnerODuplicado"
            style="@style/ComienzaSpinner"
            android:layout_below="@+id/textLogistica"
            android:layout_toEndOf="@+id/TextNDuplicado" />

        <TextView
            android:id="@+id/textN"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/TextPDuplicado"
            android:text="Insumos Completos" />

            <Spinner
                android:id="@+id/spinnnerN"
                style="@style/ComienzaSpinner"
                android:layout_below="@+id/TextPDuplicado"
                android:layout_toEndOf="@+id/textN" />

        <TextView
            android:id="@+id/textO"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/textN"
            android:text="Pendiente Regoger Insumos" />

            <Spinner
                android:id="@+id/spinnnerO"
                style="@style/ComienzaSpinner"
                android:layout_below="@+id/textN"
                android:layout_toEndOf="@+id/textO" />

        <TextView
            android:id="@+id/textP"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/textO"
            android:text="Tiempo de Espera al Ingreso" />

            <Spinner
                android:id="@+id/spinnnerP"
                style="@style/ComienzaSpinner"
                android:layout_below="@+id/textO"
                android:layout_toEndOf="@+id/textP" />

        <TextView
            android:id="@+id/textQ"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/textP"
            android:text="Timepo de Espera a la Salida" />

            <Spinner
                android:id="@+id/spinnnerQ"
                style="@style/ComienzaSpinner"
                android:layout_below="@+id/textP"
                android:layout_toEndOf="@+id/textQ" />


        <ImageView
            android:id="@+id/imageViewSeparatorSolicitudes"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textQ"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="20dp"
            android:layout_marginStart="20dp"
            android:padding="1dp"
            android:background="@color/black"
            android:contentDescription="TODO"
            android:src="@android:drawable/divider_horizontal_bright" />

        <TextView
            android:id="@+id/textSolicitudes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageViewSeparatorSolicitudes"
            android:layout_centerHorizontal="true"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:layout_marginTop="20dp"
            android:text="SOLICITUDES" />

        <TextView
            android:id="@+id/textR"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/textSolicitudes"
            android:text="Servicio a Cotizar" />

            <Spinner
                android:id="@+id/spinnnerR"
                style="@style/ComienzaSpinner"
                android:layout_below="@+id/textSolicitudes"
                android:layout_toEndOf="@+id/textQ" />

            <ImageView
                android:id="@+id/imageViewSeparator"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textR"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                android:layout_marginStart="20dp"
                android:padding="1dp"
                android:background="@color/black"
                android:contentDescription="TODO"
                android:src="@android:drawable/divider_horizontal_bright" />

            <TextView
                android:id="@+id/Generador"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageViewSeparator"
                android:layout_centerHorizontal="true"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:layout_marginTop="30sp"
                android:text="SERVICIO" />

        <TextView
            android:id="@+id/text2A"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/Generador"
            android:text="Tipo de Servicio Realizado" />

        <EditText
            android:id="@+id/EditTipoServicio"
            android:layout_width="60dp"
            android:layout_height="23dp"
            android:layout_below="@+id/Generador"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:textSize="8sp"
            android:layout_toEndOf="@+id/TextPDuplicado"
            android:background="@drawable/style_etbg"
            android:gravity="center" />

        <TextView
            android:id="@+id/text2B"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/text2A"
            android:text="Otro" />

        <EditText
            android:id="@+id/EditOtrosServicios"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/text2A"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:layout_toEndOf="@+id/text2B"
            android:padding="10dp"
            android:background="@drawable/style_etbg"
            android:gravity="center"
            android:textSize="8sp" />

        <ImageView
            android:id="@+id/Camera2"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_below="@+id/EditOtrosServicios"
            android:layout_marginStart="20dp"
            android:layout_marginTop="30dp"
            tools:src="@android:drawable/ic_menu_camera" />

        <ImageButton
            android:id="@+id/ButtonCamera2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/EditOtrosServicios"
            android:layout_marginStart="30dp"
            android:layout_marginTop="60dp"
            android:layout_marginBottom="20dp"
            android:layout_toEndOf="@+id/Camera2"
            android:shadowColor="@color/black"
            android:src="@drawable/ic_baseline_camera"
            android:text="Camara" />

        <Button
            android:id="@+id/buttonEnviar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/Camera2"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="30sp"
            android:layout_marginBottom="20sp"
            android:text="ENVIAR" />
