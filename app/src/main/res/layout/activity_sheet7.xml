            android:gravity="center" />

        <TextView
            android:id="@+id/textD"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/textC"
            android:text="OBSERVACIONES" />

        <EditText
            android:id="@+id/EditObservaciones"
