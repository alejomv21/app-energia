            android:layout_marginTop="20dp"
            android:layout_toEndOf="@+id/textH"
            android:shadowColor="@color/black"
            android:src="@drawable/ic_calendar_today"
            android:text="Camara" />


        <EditText
            android:id="@+id/EditUltimoLavado"
            android:layout_width="60dp"
            android:layout_height="23dp"
            android:layout_below="@+id/textG"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:layout_toEndOf="@+id/fechaUltimoLavado"
            android:background="@drawable/style_etbg"
            android:gravity="center"
            android:textSize="8sp" />

        <TextView
            android:id="@+id/textI"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/textH"
            android:text="Ultimo Tanqueo (gls)" />

        <ImageButton
            android:id="@+id/fechaUltimoTanqueo"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_below="@+id/textH"
            android:layout_marginStart="30dp"
            android:layout_marginTop="20dp"
            android:layout_toEndOf="@+id/textI"
            android:shadowColor="@color/black"
            android:src="@drawable/ic_calendar_today"
            android:text="Camara" />

        <EditText
            android:id="@+id/EditUltimoTanqueo"
            android:layout_width="60dp"
            android:layout_height="23dp"
            android:layout_below="@+id/textH"
            android:layout_marginStart="20dp"
            android:textSize="8sp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:layout_toEndOf="@+id/fechaUltimoTanqueo"
            android:background="@drawable/style_etbg"
            android:gravity="center" />


        <TextView
            android:id="@+id/textJ"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/textI"
            android:text="Capacidad Tanqueo" />

        <EditText
            android:id="@+id/EditCapacidadTanque"
            android:layout_width="60dp"
            android:layout_height="23dp"
