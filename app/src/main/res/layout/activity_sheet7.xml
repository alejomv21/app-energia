            android:layout_height="wrap_content"
            android:layout_below="@+id/textQ"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="20dp"
            android:layout_marginStart="20dp"
            android:padding="1dp"
            android:background="@color/black"
            android:contentDescription="@string/todo"
            android:src="@android:drawable/divider_horizontal_bright" />

        <TextView
            android:id="@+id/textSolicitudes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageViewSeparatorSolicitudes"
            android:layout_centerHorizontal="true"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:layout_marginTop="20dp"
            android:text="@string/solicitudes" />

        <TextView
            android:id="@+id/textR"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/textSolicitudes"
            android:text="@string/servicio_a_cotizar" />

            <Spinner
                android:id="@+id/spinnnerR"
                style="@style/ComienzaSpinner"
                android:textSize="8sp"
                android:layout_below="@+id/textSolicitudes"
                android:layout_toEndOf="@+id/textQ" />

        <TextView
            android:id="@+id/TextTipoServicioRealizado"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/textR"
            android:text="@string/tipo_de_servicio_realizado" />

        <Spinner
            android:id="@+id/spinnnerTipoServicioRealizado"
            style="@style/ComienzaSpinner"
            android:textSize="8sp"
            android:layout_below="@+id/textR"
            android:layout_toEndOf="@+id/textQ" />

            <ImageView
                android:id="@+id/imageViewSeparator"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/TextTipoServicioRealizado"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                android:layout_marginStart="20dp"
                android:padding="1dp"
                android:background="@color/black"
                android:contentDescription="@string/todo"
                android:src="@android:drawable/divider_horizontal_bright" />

            <TextView
                android:id="@+id/Generador"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageViewSeparator"
                android:layout_centerHorizontal="true"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:layout_marginTop="30sp"
                android:text="@string/servicio" />

        <TextView
            android:id="@+id/text2A"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/Generador"
            android:text="@string/tipo_de_servicio_realizado" />

        <EditText
            android:id="@+id/EditTipoServicio"
            android:layout_width="60dp"
            android:layout_height="23dp"
            android:layout_below="@+id/Generador"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:textSize="8sp"
            android:layout_toEndOf="@+id/TextPDuplicado"
            android:background="@drawable/style_etbg"
            android:gravity="center" />

        <TextView
            android:id="@+id/text2B"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/text2A"
            android:text="@string/otro" />

        <EditText
            android:id="@+id/EditOtrosServicios"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/text2A"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:layout_toEndOf="@+id/text2B"
            android:padding="10dp"
            android:background="@drawable/style_etbg"
            android:gravity="center"
            android:textSize="8sp" />

        <ImageView
            android:id="@+id/Camera2"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_below="@+id/EditOtrosServicios"
            android:layout_marginStart="20dp"
            android:layout_marginTop="30dp"
            tools:src="@android:drawable/ic_menu_camera" />

        <ImageButton
            android:id="@+id/ButtonCamera7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/EditOtrosServicios"
            android:layout_marginStart="30dp"
            android:layout_marginTop="60dp"
            android:layout_marginBottom="20dp"
            android:layout_toEndOf="@+id/Camera2"
            android:shadowColor="@color/black"
            android:src="@drawable/ic_baseline_camera"
            android:text="@string/camara" />

        <ImageButton
            android:id="@+id/ButtonCameraSearch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/EditOtrosServicios"
            android:layout_marginStart="30dp"
            android:layout_marginTop="60dp"
            android:layout_marginBottom="20dp"
            android:layout_toEndOf="@+id/ButtonCamera7"
            android:shadowColor="@color/black"
            android:src="@drawable/ic_baseline_image_search"
            android:text="@string/camara" />

        <ImageButton
            android:id="@+id/ButtonCamera7Upload"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/EditOtrosServicios"
            android:layout_marginStart="30dp"
            android:layout_marginTop="60dp"
            android:layout_marginBottom="20dp"
            android:layout_toEndOf="@+id/ButtonCameraSearch"
            android:shadowColor="@color/black"
            android:src="@drawable/ic_baseline_cloud_upload"
            android:text="@string/camara" />

        <Button
            android:id="@+id/buttonEnviar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/Camera2"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="30sp"
            android:layout_marginBottom="20sp"
            android:text="@string/enviar" />
    </RelativeLayout>
    </ScrollView>
</layout>