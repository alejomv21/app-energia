            android:padding="1dp"
            android:src="@android:drawable/divider_horizontal_bright" />

        <TextView
            android:id="@+id/editSeguridadTrabajo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageViewSeparatorEpp"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="20dp"
            android:text="SEGURIDAD EN EL TRABAJO"
            android:textColor="@color/black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textEpp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/editSeguridadTrabajo"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="20dp"
            android:text="EPPÂ´S"
            android:textColor="@color/black"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/textK"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/textEpp"
            android:text="CASCO" />

            <Spinner
                android:id="@+id/spinnnerK"
                style="@style/ComienzaSpinner"
                android:layout_below="@+id/textEpp"
                android:layout_toEndOf="@+id/textK" />

        <TextView
            android:id="@+id/textL"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/textK"
            android:text="GUANTES" />

            <Spinner
                android:id="@+id/spinnnerL"
                style="@style/ComienzaSpinner"
                android:layout_below="@+id/textK"
                android:layout_toEndOf="@+id/textL" />

        <TextView
            android:id="@+id/textM"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/textL"
            android:text="OVEROL" />

            <Spinner
                android:id="@+id/spinnnerM"
                style="@style/ComienzaSpinner"
                android:layout_below="@+id/textL"
                android:layout_toEndOf="@+id/textM" />

        <TextView
            android:id="@+id/TextNDuplicado"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/textM"
            android:text="GAFAS" />
            <Spinner
                android:id="@+id/spinnnerNDuplicado"
                style="@style/ComienzaSpinner"
                android:layout_below="@+id/textM"
                android:layout_toEndOf="@+id/TextNDuplicado" />

        <TextView
            android:id="@+id/TextODuplicado"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/TextNDuplicado"
            android:text="OTROS" />

        <EditText
            android:id="@+id/EditOtros"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/TextNDuplicado"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:padding="5dp"
            android:layout_toEndOf="@+id/TextPDuplicado"
            android:background="@drawable/style_etbg"
            android:gravity="center"
            android:textSize="8sp" />


        <TextView
            android:id="@+id/EditDocumentos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/EditOtros"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="20dp"
            android:text="DOCUMENTOS"
            android:textColor="@color/black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textAtsTrealizar"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/EditDocumentos"
            android:text="Diligenciamiento de ATS del trabajos a realizar" />

        <Spinner
            android:id="@+id/spinnnerAtsTrealizar"
            style="@style/ComienzaSpinner"
            android:layout_below="@+id/EditDocumentos"
            android:layout_toEndOf="@+id/textAtsTrealizar" />

        <TextView
            android:id="@+id/textAtsTalturas"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/textAtsTrealizar"
            android:text="Diligenciamiento de ATS de trabajos en Alturas" />

        <Spinner
            android:id="@+id/spinnnerAtsTalturas"
            style="@style/ComienzaSpinner"
            android:layout_below="@+id/textAtsTrealizar"
            android:layout_toEndOf="@+id/textAtsTalturas" />

        <TextView
            android:id="@+id/textAtsTespaciosConfinados"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/textAtsTalturas"
            android:text="Diligenciamiento de ATS de trabajos en espacios confinados" />

        <Spinner
            android:id="@+id/spinnnerAtsTConfinados"
            style="@style/ComienzaSpinner"
            android:layout_below="@+id/textAtsTalturas"
            android:layout_toEndOf="@+id/textAtsTespaciosConfinados" />

        <TextView
            android:id="@+id/textAtscaliente"
            style="@style/ComienzaTexto"
            android:layout_below="@+id/textAtsTespaciosConfinados"
            android:text="Diligenciamiento de ATS de trabajos en caliente" />

        <Spinner
            android:id="@+id/spinnnerAtscaliente"
            style="@style/ComienzaSpinner"
            android:layout_below="@+id/textAtsTespaciosConfinados"
            android:layout_toEndOf="@+id/textAtscaliente" />


        <ImageView
            android:id="@+id/imageViewSeparatorLogistica"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
